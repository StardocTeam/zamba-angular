            } else if (document.getElementById("zamba_index_2694").value == "") {
                alert("Por favor, verifique el Nro de Subsiniestro antes de continuar");
                document.getElementById("hdnRuleId").name = "zamba_rule_cancel";
            } else if (document.getElementById("zamba_index_14").value == "") {
                alert("Por favor, verifique el Nro de Item antes de continuar");
                document.getElementById("hdnRuleId").name = "zamba_rule_cancel";
            } else if (document.getElementById("zamba_index_2701").value == "") {
                alert("Por favor, verifique la Carátula antes de continuar");
                document.getElementById("hdnRuleId").name = "zamba_rule_cancel";
            } else if (document.getElementById("zamba_index_34").value == "") {
                alert("Por favor, verifique el Nro de Póliza antes de continuar");
                document.getElementById("hdnRuleId").name = "zamba_rule_cancel";
            } else if (document.getElementById("zamba_index_2683").value == "") {
                alert("Por favor, verifique la Fecha de Ocurrencia antes de continuar");
                document.getElementById("hdnRuleId").name = "zamba_rule_cancel";
            } else if ($('#zamba_index_2742').val() == "S" && $('#zamba_index_1000002').val() == null) {
                alert("Por favor, verifique el Motivo de Rechazo antes de continuar");
                document.getElementById("hdnRuleId").name = "zamba_rule_cancel";
            } else if (document.getElementById("zamba_index_2682").value == "") {
                alert("Por favor, verifique la Primera Fecha de Notificación antes de continuar");
                document.getElementById("hdnRuleId").name = "zamba_rule_cancel";
            } else if ($('input[type=text]').hasClass('error')) {
                alert("Verifique que todos los datos de los campos sean válidos");
                document.getElementById("hdnRuleId").name = "zamba_rule_cancel";
            } else {
                document.getElementById("hdnRuleId").name = "zamba_rule_save";
            }
        }
