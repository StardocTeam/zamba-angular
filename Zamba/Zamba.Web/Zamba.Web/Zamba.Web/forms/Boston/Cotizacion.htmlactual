        function zamba_save_onclick(sender) {
            var mensaje = MensajeError();
            if (mensaje != "") {
                alert(mensaje[0]);
                document.getElementById("hdnRuleId").name = sender.id;
            } else {
                alert("Se guardó de manera exitosa.");
                document.getElementById("hdnRuleId").name = sender.id;
                frmMain.submit();
            }
        }

        function MensajeError() {
            var mensaje = "";
            if (document.getElementById("zamba_index_95").value.trim() == "") {
                mensaje = ["La Fecha de Ingreso a la Compañía es un dato obligatorio.", "zamba_index_95"];
            } else if (document.getElementById("zamba_index_94").value.trim() == "") {
                mensaje = ["El Ejecutivo comercial es un dato obligatorio.", "zamba_index_94"];
            } else if (document.getElementById("zamba_index_32").value.trim() == "") {
                mensaje = ["La Seccion es un dato obligatorio.", "zamba_index_32"];
            } else if (document.getElementById("zamba_index_4").value.trim() == "") {
                mensaje = ["El Código de Productor es un dato obligatorio.", "zamba_index_4"];
            } else if (document.getElementById("zamba_index_60").value.trim() == "") {
                mensaje = ["La Agencia es un dato obligatorio.", "zamba_index_60"];
            } else if (document.getElementById("zamba_index_104").value.trim() == "") {
                mensaje = ["El Grupo es un dato obligatorio.", "zamba_index_104"];
            } else if (document.getElementById("zamba_index_1128").value.trim() == "" && document.getElementById("zamba_index_2710").value.trim() == "") {
                mensaje = ["Debe cargar el CUIT o el DNI.", "zamba_index_1128"];
            } else if (document.getElementById("zamba_index_98").value.trim() == "") {
                mensaje = ["Plazo Limite Cotizacion es un dato obligatorio.", "zamba_index_98"];
            } else if (document.getElementById("zamba_index_3").value.trim() == "") {
                mensaje = ["El Asegurado es un dato obligatorio.", "zamba_index_3"];
            } else if (document.getElementById("zamba_index_100").value.trim() == "") {
                mensaje = ["El Suscriptor es un dato obligatorio.", "zamba_index_100"];
            } else if (invertirFecha($("#zamba_index_95").val()) > invertirFecha($("#zamba_index_96").val())) {
                mensaje = ["La fecha de Ingreso a la Compañía debe de ser menor o igual a la Fecha de Ingreso a Zamba."];
            } else if (invertirFecha($("#zamba_index_98").val()) < invertirFecha($("#zamba_index_96").val())) {
                mensaje = ["Plazo Limite de Cotización debe de ser mayor o igual a la Fecha de Ingreso a Zamba."];
            }
            return mensaje;
        }

        function invertirFecha(mifecha) {
            var fecha = mifecha.split(/\//);
            var fecha = mifecha.split(/\//).reverse().join('/');
            return fecha;
        }

        function BuscarProductor(sender) {

            document.getElementById("hdnRuleId").name = sender.id;
            frmMain.submit();

        }

        function zamba_onclick(sender) {
            document.getElementById("hdnRuleId").name = sender.id;
            frmMain.submit();
        }

        function DeshabilitarCamposBusqueda() {
            if ($("#zamba_index_4").val() != "" && $("#zamba_index_58").val() != "" && $("#zamba_index_94").val() != "") {
                if ($("#zamba_index_60").val() != "" && $("#zamba_index_104").val() != "") {

                    $("#zamba_index_60").attr('disabled', true);
                    $("#zamba_index_104").attr('disabled', true);
                }
            }
            if ($('#zamba_index_94').val() == '' || $('#zamba_index_94').val() == null || $('#zamba_index_94').val() == 0) {
                $("#zamba_index_94").attr('disabled', false);
            } else { $("#zamba_index_94").attr('disabled', true); }
        }

        function LimpiarCamposBusqueda() {
            $("#zamba_index_4").val("");
            $("#zamba_index_58").val("");
            $("#zamba_index_94").val("");
            $("#zamba_index_60").val("");
            $("#zamba_index_104").val("");
        }

        function FuncionesPosCargaDeReclamo() {
            CargarFechasyBotones2();
            DeshabilitarCamposBusqueda();
            MostrarPolizaSegunMovimiento();


            if ($("#zamba_index_32").val() == null)
                $("#zamba_index_32").val("0");

            if ($("#zamba_index_38").val() == null)
                $("#zamba_index_38").val("2");

            var agencia = $("#zamba_index_60").val();
            var seleccionado = $("#zamba_index_1001006").val();


            $(".emisor option[value!=1][value!=" + agencia + "]")
                .remove()
                .end()
                .val(seleccionado);
        }

        function HabilitarCampos() {

            if ($("#zamba_index_94").val() == '' || $("#zamba_index_94").val() == null) {
                $("#zamba_index_94").attr('disabled', false);
            }

            if ($("#zamba_index_60").val() == '' || $("#zamba_index_60").val() == null) {
                $("#zamba_index_60").attr('disabled', false);
            }

            if ($("#zamba_index_104").val() == '' || $("#zamba_index_104").val() == null) {
                $("#zamba_index_104").attr('disabled', false);
            }
        }

        function MostrarPolizaSegunMovimiento() {
            if ($('#zamba_index_38').val() == 2 || $('#zamba_index_38').val() == 3) {
                $('#divpoliza').show();
            } else {
                $('#divpoliza').hide();
            }
        }

        function MostrarIDPymeSegunResguardo() {
            if ($('#zamba_index_154').val() == "S") {
                $('#divpyme').show();
            } else {
                $('#divpyme').hide();
            }
        }

        function CargarFechasyBotones2() {
            $(".solonums").each(function () {
                $(this).keypress(function (e) {
                    if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                        return false;
                    }
                })
            });

            if ($('#zamba_index_155').val() == null) {
                $('#zamba_index_155').hide();
            }
            if ($('#zamba_index_155').val() != null) {
                $('#zamba_index_155').show();
            }
            if ($('#zamba_index_154').val() != null) {
                $('#zamba_index_154').attr('disabled', true);
            }
            if ($('#zamba_index_154').val() == null) {
                $('#zamba_index_154').attr('disabled', false);
            }

            /* $('#zamba_index_98').datepicker({
                changeMonth: true,
                changeYear: true,
                showOn: 'button',
                buttonText: 'Abrir calendario',
                buttonImage: 'calendar.png',
                buttonImageOnly: true,
                yearRange: '-100:+0',
                duration: ""
            });
            $('#zamba_index_95').datepicker({
                changeMonth: true,
                changeYear: true,
                showOn: 'button',
                buttonText: 'Abrir calendario',
                buttonImage: 'calendar.png',
                buttonImageOnly: true,
                yearRange: '-100:+0',
                duration: ""
            });
*/
            MostrarIDPymeSegunResguardo();

            if ($('#zamba_index_94').val() == '' || $('#zamba_index_94').val() == null) {
                $("#zamba_index_94").val(0);
            }
            if ($('#zamba_index_38').val() == '' || $('#zamba_index_38').val() == null) {
                $("#zamba_index_38").val(0);
            }
            if ($('#zamba_index_94').val() == '' || $('#zamba_index_94').val() == null || $('#zamba_index_94').val() == 0) {
                $("#zamba_index_94").attr('disabled', false);
            } else { $("#zamba_index_94").attr('disabled', true); }
        }
        window.onload = function () {
            setTimeout(function () { FuncionesPosCargaDeReclamo(); }, 500);
//Si tiene documento embebido, muestra el scroll			
			setIframeScrollAttribute();
			SetInitialConfigurations();
        };
