        <script type="text/javascript" language="JavaScript">
            function zamba_save_onclick(sender) {
                var valido = true;
                if (document.getElementById("zamba_index_2").value == "") {
                    swal("", "Por favor, ingrese el CUIT", "error");
                    valido = false;
                } else if (document.getElementById("zamba_index_105").value == "") {
                    swal("", "Por favor, ingrese el Nombre y Apellido o Razón Social", "error");
                    valido = false;
                } else if (document.getElementById("zamba_index_72").value == "") {
                    swal("", "Por favor, ingrese el Punto de venta", "error");
                    valido = false;
                } else if (document.getElementById("zamba_index_10").value == "") {
                    swal("", "Por favor, ingrese el Nro de Comprobante", "error");
                    valido = false;
                } else if (document.getElementById("zamba_index_50").value == "") {
                    swal("", "Por favor, ingrese la Orden de Compra Nro:", "error");
                    valido = false;
                } else if (document.getElementById("zamba_index_16").value == "") {
                    swal("", "Por favor, ingrese el Nro de Solicitud:", "error");
                    valido = false;
                } else if (document.getElementById("zamba_index_82").value == "") {
                    swal("", "Por favor, ingrese la delegacion", "error");
                    valido = false;
                } else if (document.getElementById("zamba_index_126").value == "") {
                    swal("", "Por favor, ingrese el Sector", "error");
                    valido = false;
                } else if (document.getElementById("zamba_index_53").value == "") {
                    swal("", "Por favor, ingrese el Solicitante", "error");
                    valido = false;
                } else if (document.getElementById("zamba_index_160").value == "") {
                    swal("", "Por favor, ingrese el Recepcionado", "error");
                    valido = false;

                } else {
                    SetRuleId(sender);
                }
                return valido;
            }
